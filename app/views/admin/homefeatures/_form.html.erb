<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
       Add New Feature
     </div>
     <div class="panel-body">
      <div class="row">
     <%= form_for [:admin, @admin_homefeature] do |f| %>
        <div class="col-md-6">

          <div class="form-group">
            <%=f.label :title%>
            <%=f.text_field :title ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :subtitle%>
            <%=f.text_field :subtitle ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :link_text%>
            <%=f.text_field :link_text ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :link_path%>
            <%=f.text_field :link_path ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :testimonila_name%>
            <%=f.text_field :testimonila_name ,class: 'form-control', placeholder: 'testimonila_name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :testimonial_remarks%>
            <%=f.text_field :testimonila_remarks ,class: 'form-control', placeholder: 'testimonila_remarks', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :link_path%>
            <%=f.text_field :link_path ,class: 'form-control', placeholder: 'Enter name', required: 'true'%>
          </div>
          <div class="form-group">
            <%=f.label :image%>
            <%=f.file_field :image ,class: 'form-control', placeholder: 'Enter name'%>
          </div>
          <div class="form-group">
            <%=f.label :Image_text%>
            <%=f.text_field :image_text ,class: 'form-control', placeholder: 'Enter Image Text', required: 'true'%>
          </div>
          <div class="form-group">
          <%=f.label :testimonial_image%>
            <%=f.file_field :testimonial_image ,class: 'form-control', placeholder: 'Enter name'%>
          </div>

          <%= f.submit 'Submit Button', class: "btn btn-default" %>
          <button type="reset" class="btn btn-default">Reset Button</button>
          <a  href="/admin/users" class="btn btn-default">Back</a>
        </div>

        <%end%>
      </div>
    </div>
  </div>
</div>
</div>
